<?xml version="1.0" encoding="utf-8" ?>

<permissions>
    <!-- Uses the Doze architecture of MotoDisplay. -->
    <feature name="com.motorola.motodisplay.doze.glimpse"/>

    <!-- Feature for 2017 products. -->
    <feature name="com.motorola.motodisplay.6.0"/>

    <!-- Add restriction to not reinstall MotoDisplay for work profile-->
    <disallowed-managed-profile package="com.motorola.motodisplay"/>
</permissions>

